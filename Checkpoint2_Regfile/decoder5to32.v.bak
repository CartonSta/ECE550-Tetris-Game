
module decoder2to4(in,out);

    intput [1:0] in;
	 output [3:0] out;
	 wire [1:0] not_in;
	 
	 not not1(not_in[0],in[0]);
	 not not2(not_in[1],in[1]);
	 and and1(out[3],in[0],in[1]);
	 and and2(out[2],not_in[0],in[1]);
	 and and3(out[1],in[0],not_in[1]);
	 and and4(out[0],not_in[0],not_in[1]);
	 
endmodule

module decoder5to32(in,out);

    input [4:0] in;
    output [32:0] out;


endmodule